CREATE FUNCTION create_items(group_number integer, in_erecord integer) RETURNS integer AS $$
DECLARE
    i RECORD;
BEGIN
    FOR i IN SELECT * FROM student WHERE i.id_groupp = group_number LOOP
		INSERT INTO ierecord (srating, erating, trating, id_erecord, id_student) VALUES (NULL, NULL, NULL, in_erecord, i.id_student);
    END LOOP;

    RETURN 1;
END;
$$ LANGUAGE plpgsql;

